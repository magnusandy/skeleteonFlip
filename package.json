{
  "name": "excalibur-webpack",
  "version": "1.0.0",
  "private": true,
  "description": "a webpack starter for excaliburjs projects",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server --env.mode development --open",
    "dev": "npm run webpack-dev-server",
    "start": "npm run dev",
    "build:dev": "npm run webpack -- --env.mode development",
    "build:prod": "npm run webpack -- --env.mode production && rm -rf docs/ && mv dist docs && rm docs/*.gz",
    "cordova:update": "yarn build:prod && cd cordovaFlip && rm -rf www/ && rm -rf /platform/android && cp -R ../docs www",
    "cordova:android-build": "cd cordovaFlip && cordova build android",
    "cordova:android-full": "yarn cordova:update && yarn cordova:android-build"
  },
  "author": "Excalibur.js Team",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@types/node": "10.5.6",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.11",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "^5.5.0",
    "source-map-loader": "0.2.4",
    "ts-loader": "4.3.0",
    "typescript": "^3.4.3",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "dependencies": {
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-modal": "^3.8.2",
    "excalibur": "0.22.0",
    "html-webpack-root-plugin": "^0.10.0",
    "install": "^0.12.2",
    "java8script": "^1.1.1",
    "localforage": "^1.7.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.1"
  }
}
